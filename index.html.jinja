{% extends 'sidemenu.html.jinja' %}

{% block styles %}
{{ super() }}
<link href="styles/mainBackground.css" rel="stylesheet" type="text/css" />
<link href="styles/mainTopbar.css" rel="stylesheet" type="text/css" />
<link href="styles/mainPosts.css" rel="stylesheet" type="text/css" />
{% endblock %}

{% block preloads %}
<link rel="preload" href="/get_posts/home" as="fetch" type="application/json" crossorigin="anonymous">
{% endblock %}

{% block content %}
<div class="search-bar" id="search-bar">
    <p>Search</p>
</div>

<div class="posts" id="posts">
</div>
{% endblock %}

{% block body %}
    {{ super() }}
    <script src="home.js"></script>
    <script src="/scripts/generatePost.js"></script>
    <script src="/scripts/formatNumbers.js"></script>
{% endblock %}